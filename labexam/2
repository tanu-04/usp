#include<stdio.h>
#include<unistd.h>
#include<fcntl.h>
#include<sys/stat.h>
int main(int argc, char *argv[]	){
	mode_t new_umask;
	struct stat stat1;

	printf("Ã«netr octal umask\n");
	scanf("%o", &new_umask);

	umask(new_umask);
	int fd =open(argv[1], O_WRONLY, 0777);
	close(fd);

	stat(argv[1], &stat1);
	printf("%d\n", stat1.st_mode);




	chmod(argv[1], 0777);
	stat(argv[1], &stat1);
	printf("%d\n", stat1.st_mode);

	return 0;}
